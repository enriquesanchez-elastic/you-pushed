{
  "name": "you-pushed",
  "displayName": "You Pushed",
  "description": "Dark Souls style 'YOU PUSHED' message when pushing to git",
  "version": "0.0.1",
  "publisher": "souls-dev",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/.git"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "you-pushed.test",
        "title": "You Pushed: Test Animation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}

